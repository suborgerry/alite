<section
  id="MainProduct-{{ section.id }}"
  class="page-width section-{{ section.id }}-padding"
  data-section="{{ section.id }}"
>
  {{ 'section-main-product.css' | asset_url | stylesheet_tag }}
  {{ 'component-accordion.css' | asset_url | stylesheet_tag }}
  {{ 'component-price.css' | asset_url | stylesheet_tag }}
  {{ 'component-slider.css' | asset_url | stylesheet_tag }}
  {{ 'component-rating.css' | asset_url | stylesheet_tag }}
  {{ 'component-loading-overlay.css' | asset_url | stylesheet_tag }}
  {{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}
  {{ 'discount-popup.css' | asset_url | stylesheet_tag }}
{{ 'cart-popup.css' | asset_url | stylesheet_tag }}
<script src="{{ 'discount-popup.js' | asset_url }}"></script>
<script src="{{ 'cart-popup.js' | asset_url }} "></script>
  {% for c in product.collections %}
    {% if c.metafields.custom.country_year or c.metafields.custom.state_year  %}
      {% assign curCol = c %}
    {% endif %}
    {% endfor %}
  {%- style -%}
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
      padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
    }

    @media screen and (min-width: 750px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding_top }}px;
        padding-bottom: {{ section.settings.padding_bottom }}px;
      }
    }
  {%- endstyle -%}

  <style>
    .param_name {
    font-size: 15px;
}
      .scroll_year-block {
        height: 100%;
        flex-shrink: 0;
        display: grid;
        padding: 18px;
        border: 1px rgba(0, 0, 0, 0.2) solid;
        border-radius: 18px;
        background-color: #f4f4f4;
        grid-row: 1 / 3;
        grid-column: 3;
      }
    @media screen and (max-width:1000px){
      
  .scroll_year-block{
    grid-row: 2 / 4;
    grid-column: 2;
  }
      .product_add_info {
    gap: 4px;
}
      .product_param {
    display: flex;
    gap: 12px;
    padding: 15px 12px;
    text-wrap: nowrap;
}
}
      .active .icon {
        opacity:1;
        display: flex;
        align-items: center;
      }
      .active {
        background-color: #DA9DFF;
        border: 1px solid #2e2e2e;
      }
      .button-active {
        background-color: #000000;
        cursor:pointer;
      }
      .active .scroll_year-block_image {
        box-shadow: rgba(0, 0, 0, 0.09) 0px 2px 1px,
          rgba(0, 0, 0, 0.09) 0px 4px 2px, rgba(0, 0, 0, 0.09) 0px 8px 4px,
          rgba(0, 0, 0, 0.09) 0px 16px 8px, rgba(0, 0, 0, 0.09) 0px 32px 16px;
      }
      .scroll_year-block_content {
        display: flex;
        gap:30px;
        justify-content: space-between;
        align-items: center;
        margin: 0 0 15px 0;
      }
         {% if curCol.metafields.custom.number_texture %} 
            .scroll_year-block_content {
        margin: 0 0 5px 0;
      }
            {% endif %}
        .texture{
          color:#000;
          margin:0 0 10px 0;
          font-size:15px;
        }
    .texture span{
      font-weight:700;
    }
      .year_title {  
        margin: 0;
        font-size: 15px;
        font-weight: 800;
      }
      .scroll_year-block_image {
        display: flex;
        border-radius: 5px;
        overflow: hidden;
        align-self:end;
      }
      .scroll_year-block_image img {
        width: 100%;
      }
    .ts-min-order {
      display: inline-flex;
      margin: 10px 0;
      font-family:"ShopifySans", sans-serif;
    }

    .ts-min-order__badge {
      display: flex;
      align-items: center;
      justify-content: center;
      background: #FFEFEF;
      width: fit-content;
      height: 38px;
      border-radius: 10px;
      gap: 10px;
      padding: 0 16px;
      font-weight: 700;
      font-family: "ShopifySans";
      color: #000;
      margin: 0px;
      cursor: pointer;
    }

    .ts-min-order__icon {
      display: flex;
      border-radius: 30px;
      box-shadow: 0px 0px 11px 0px #E0103A4F;
    }

    .ts-min-order__icon svg {
      width: 16px;
      height: 16px;
    }

    .ts-min-order__badge:hover + .ts-min-order__tooltip {
      opacity: 1;
    }

    .ts-min-order__tooltip {
      position: absolute;
      bottom: calc(100% + 10px);
      left: 0;
      width: 120%;
      max-width: 210px;
      padding: 8px 12px;
      background: #fff;
      border: 1px solid #D9D9D9;
      border-radius: 10px;
      pointer-events: none;
      user-select: none;
      transition: opacity 0.3s;
      opacity: 0;
      z-index: 1;
    }

    .ts-min-order__tooltip * {
      margin: 0;
      padding: 0;
      list-style: none;
      line-height: 1.5;
      font-size: 14px;
    }

    .ts-min-order__tooltip::after {
      content: url("data:image/svg+xml,%3Csvg width='12' height='6' viewBox='0 0 16 8' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='M8.5 7C8.26172 7 2 1 2 1H14.5C14.5 1 8.73828 7 8.5 7Z' fill='%23D9D9D9' stroke='%23D9D9D9'/%3E%3C/svg%3E%0A");;
      position: absolute;
      top: 100%;
      left: 10px;
      font-size: 0;
      line-height: 1;
    }

    .ts-badges {
      position: relative;
    }

    .ts-badges__container {
      display: flex;
      gap: 10px;
      overflow: auto;
      -ms-overflow-style: none;  /* IE and Edge */
      scrollbar-width: none; 
    }
  
    .ts-badges__container::-webkit-scrollbar {
      display: none;
    }

    .ts-badges .ts-min-order,
    .ts-badges .freeshippingbadge,
    .ts-badges .product__inventory {
      margin: 0;
      min-width: fit-content;
    }
    .freeshippingbadge {
    background-color: #e7f4ff !important;
  }
    .freeshippingbadge, .ts-min-order__badge,.product__inventory{
          border-radius: 12px !important;;
    }
    @media screen and (max-width: 749px) {
      .ts-min-order__tooltip {
        width: 125%;
      }
    }
  </style>
<style>
{% if template == 'product.blackout-eu' or template == 'product.blackout-eu2' or template == 'product.blackout-us2' or template == 'product.blackout' or template == 'product.flipper-new' or template == 'product.flipper-new-both' or template == 'product.flipper-new-hard' or template == 'product.material-new'%}
@media(max-width:1000px){
  .product_add_info{
    grid-template-columns: 1fr 1fr;
    grid-template-rows: 1.5fr 1.5fr 2.5fr;
  }
}
{% else %}
@media(max-width:1000px){
  .product_add_info{
    grid-template-columns: 1fr 1fr;
    grid-template-rows: 1fr 1fr 1fr;
  }
}
  {% endif %}
</style>
  {% unless section.settings.show_scroll_block %}
  {%- style -%}
    @media (max-width: 1000px) {
      .product_add_info {
        display: grid;
        grid-template-columns: 1fr 1fr;
        grid-template-rows: 1.5fr 1.5fr 1.5fr;
      }
    }
  {%- endstyle -%}
{% endunless %}


  <script src="{{ 'product-info.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
  {% if section.settings.image_zoom == 'hover' %}
    <script id="EnableZoomOnHover-main" src="{{ 'magnify.js' | asset_url }}" defer="defer"></script>
  {% endif %}
  {%- if request.design_mode -%}
    <script src="{{ 'theme-editor.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {%- assign first_3d_model = product.media | where: 'media_type', 'model' | first -%}
  {%- if first_3d_model -%}
    {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}
 {% if settings.show_breadcrumb_nav %}
  {% include 'breadcrumbs' %}
{% endif %}
  {% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}

  <div class="product product--{{ section.settings.media_size }} product--{{ section.settings.media_position }} product--{{ section.settings.gallery_layout }} product--mobile-{{ section.settings.mobile_thumbnails }} grid grid--1-col {% if product.media.size > 0 %}grid--2-col-tablet{% else %}product--no-media{% endif %}">
    <div class="grid__item product__media-wrapper{% if section.settings.media_position == 'right' %} medium-hide large-up-hide{% endif %}">
      {% render 'product-media-gallery', variant_images: variant_images %}
    </div>
    {% if section.settings.enable_add_info %}
   <div class="product_add_info grid-params">
    <!-- Всегда отображаемые параметры -->
    <div class="product_param">
      <div class="param_logo">{{ section.settings.alt_size_icon_svg }}</div>
      <div class="param_text">
        <h3 class="param__name">{{ section.settings.custom_param_height }}</h3>
        <p class="param_info">{{ curCol.metafields.custom.height }}</p>
      </div>
    </div>
    <div class="product_param">
      <div class="param_logo">{{ section.settings.alt_lifespan_icon_svg }}</div>
      <div class="param_text">
        <h3 class="param__name">{{ section.settings.custom_param_lifespan }}</h3>
        <p class="param_info">{{ curCol.metafields.custom.lifespan }}</p>
      </div>
    </div>
    <div class="product_param">
      <div class="param_logo">
        {% if curCol.metafields.custom.suitable_for == "Car" or curCol.metafields.custom.suitable_for == "Rear" or curCol.metafields.custom.suitable_for == "Front" or curCol.metafields.custom.suitable_for == "Both" or curCol.metafields.custom.suitable_for == "Carro" %}
          {% render "suitable-logo" %}
        {% elsif curCol.metafields.custom.suitable_for == "Moto"%}
          {% render "suitable-for-moto" %}
        {% else %}
          {% render "any_plate" %}
        {% endif %}
      </div>
      <div class="param_text">
        <h3 class="param_name">{{ section.settings.param_suitable_title }}</h3>
        <p class="param_info">{{ curCol.metafields.custom.suitable_for }}</p>
      </div>
    </div>
    <div class="product_param">
      <div class="param_logo">{{ section.settings.alt_color_icon_svg }}</div>
      <div class="param_text">
       <h3 class="param_name">{{ section.settings.param_color_title }}</h3>
        <p class="param_info">{{ section.settings.param_color }}</p>
      </div>
    </div>

    <!-- Переключаемый блок -->
    {% if section.settings.show_scroll_block %}
      {% if curCol != blank %}
        <div class="scroll_year-block active">
          <div class="scroll_year-block_content">
            <h3 class="year_title">
              {% if curCol.metafields.custom.state_year %}
                {{ curCol.metafields.custom.state_year }}
              {% else %}
                {{ curCol.metafields.custom.country_year }}
              {% endif %}
            </h3>
          </div>
          {% if curCol.metafields.custom.number_texture %}
            {% if template == 'product.flipper-new' or template == 'product.flipper-new-hard' or template == 'product.flipper-new-both' %}
              <p class="texture"><span>{{ curCol.metafields.custom.number_texture }}</span></p>
            {% else %}
              <p class="texture">{{ 'products.product.Design' | t }} <span>{{ curCol.metafields.custom.number_texture }}</span></p>
            {% endif %}
          {% endif %}
          <div class="scroll_year-block_image">
            <img src="{{ curCol.featured_image | image_url: width: c.featured_image.width }}" />
          </div>
        </div>
      {% endif %}
    {% else %}
      <!-- Альтернативные 2 кастомных параметра -->
      <div class="product_param">
        <div class="param_logo">{{ section.settings.alt_param1_icon_svg }}</div>
        <div class="param_text">
          <h3 class="param__name">{{ section.settings.alt_param1_title }}</h3>
          <p class="param_info">{{ section.settings.alt_param1_desc }}</p>
        </div>
      </div>
      <div class="product_param">
        <div class="param_logo">{{ section.settings.alt_param2_icon_svg }}</div>
        <div class="param_text">
          <h3 class="param__name">{{ section.settings.alt_param2_title }}</h3>
          <p class="param_info">{{ section.settings.alt_param2_desc }}</p>
        </div>
      </div>
    {% endif %}

    <p style="display:none;">template : {{ template }}</p>

    <style>
      {% if template contains 'product.flipper-new' %}
        .scroll_year-block_image {
          box-shadow: none !important;
        }
      {% endif %}
    </style>
  </div>
      {% endif %}
    <div class="product__info-wrapper grid__item{% if settings.page_width > 1400 and section.settings.media_size == "small" %} product__info-wrapper--extra-padding{% endif %}">
      <product-info
        id="ProductInfo-{{ section.id }}"
        data-section="{{ section.id }}"
        data-url="{{ product.url }}"
        class="product__info-container{% if section.settings.enable_sticky_info %} product__column-sticky{% endif %}"
      >
                
        {%- assign product_form_id = 'product-form-' | append: section.id -%}

        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when 'badges' -%}
              <div class="ts-badges">
                <div class="ts-badges__container">
            {%- if block.settings.show_min_order -%}
                  <div class="ts-min-order">
                    <div class="ts-min-order__badge">
                      <span class="ts-min-order__icon"> 
                        <svg width="21" height="21" viewBox="0 0 21 21" fill="none" xmlns="http://www.w3.org/2000/svg">
